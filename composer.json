{
  "name": "eossa/artisan-clean-architecture-boilerplate",
  "description": "A Composer package providing custom Laravel Artisan commands to streamline the creation of various clean architecture components in a Laravel application.",
  "type": "library",
  "homepage": "https://github.com/eossa/artisan-clean-architecture-boilerplate",
  "license": "MIT",
  "authors": [
    {
      "name": "Elkin Ossa",
      "email": "eossa93@hotmail.com",
      "role": "Owner"
    }
  ],
  "require": {
    "php": "^7.0",
    "illuminate/console": "^5.5",
    "illuminate/support": "^5.5"
  },
  "require-dev": {
    "orchestra/testbench": "~3.5",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "EOssa\\ArtisanCleanArchitectureBoilerplate\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "release": "composer run test && npx standard-version",
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "extra": {
    "laravel": {
      "providers": [
        "EOssa\\ArtisanCleanArchitectureBoilerplate\\CommandServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
